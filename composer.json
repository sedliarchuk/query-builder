{
    "name" : "sedliarchuk/query-builder",
    "description" : "Symfony REST Design filters",
    "version" : "1.0.0",
    "type" : "symfony-bundle",
    "homepage": "https://github.com/sedliarchuk/query-builder",
    "license" : "MIT",
    "keywords" : ["filter", "query builder bundle", "doctrine", "symfony", "request"],
    "authors" : [{
        "name" : "Sedliarchuk Sergey",
        "email" : "sedliarchuk@gmail.com"
    }],
    "require" : {
        "php": ">=7.1",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3",
        "symfony/http-kernel": "^3.3",
        "doctrine/orm": "^2.5",
        "babdev/pagerfanta-bundle": "^2.8",
        "willdurand/hateoas": "2.10",
        "ext-json": "*",
        "ext-mbstring": "*"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload":{
        "psr-4":{
            "Sedliarchuk\\QueryBuilder\\": "src/"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
